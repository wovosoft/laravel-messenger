{
    "name": "wovosoft/laravel-messenger",
    "description": "A package that adds the features of Real Time Messaging",
    "license": "MIT",
    "keywords": [
        "Laravel",
        "Messenger",
        "Chat",
        "Real Time Chatting"
    ],
    "type": "library",
    "authors": [
        {
            "name": "Narayan Adhikary",
            "email": "narayanadhikary24@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Wovosoft\\LaravelMessenger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wovosoft\\LaravelMessenger\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpunit": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wovosoft\\LaravelMessenger\\ServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
